{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "white",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": "({{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }})",
          "type": "python"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "red",
          "style": "plain",
          "template": " % ",
          "type": "root"
        },
        {
          "style": "plain",
          "template": "<magenta>{{ .UserName }}</><white>@</><yellow>{{ .HostName }}</>",
          "type": "session"
        },
        {
          "foreground": "lightYellow",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<white>: </><cyan>{{ .Path }} </>",
          "type": "path"
        },
        {
          "style": "plain",
          "template": "<green>{{ .HEAD }} </>{{ if .Working.Changed }}<red>âš¡</>{{ end }} ",
          "type": "git",
          "properties": {
            "fetch_status": true
          }
        },
        {
          "foreground": "red",
          "style": "plain",
          "template": " C:{{ if gt .Code 0 }}{{ .Code }}{{ end }} ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "white",
          "style": "plain",
          "template": "$",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "green",
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "final_space": true,
  "version": 2
}